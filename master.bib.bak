  language	= {English},
  number	= {3},
  pages		= {pp. 415-449},
  publisher	= {Blackwell Publishing on behalf of The International
		  Studies Association},
  title		= {{Foreign Policies of {A}frican States in Asymmetrical
		  Dyads}},
  volume	= {24},
  year		= {1980}
}

