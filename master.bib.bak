@Article{	  huth:1988,
  abstract	= {Successful deterrence, it is argued, requires a
		  combination of military capabilities and bargaining
		  behavior that enhances a defender's credibility without
		  provoking a potential attacker. Hypotheses on the political
		  and military conditions under which extended-immediate
		  deterence is likely to succeed or fail are formulated and
		  tested by probit analysis on fifty-eight historical cases.
		  The empirical results indicate that (1) the military
		  capability of the defender to deny the potential attacker a
		  quick and decisive victory on the battlefield enhances
		  deterrence; (2) a policy of reciprocity in diplomacy and
		  military actions by the defender contributes strongly to
		  deterrence success; and (3) a past record of backing down
		  under pressure or intransigence in confrontations with the
		  potential attacker increases the likelihood of deterrence
		  failure.},
  author	= {Huth, Paul K.},
  copyright	= {Copyright {\copyright} 1988 American Political Science
		  Association},
  issn		= {00030554},
  journal	= {The American Political Science Review},
  jstor_articletype={primary_article},
  jstor_formatteddate={Jun., 1988},
  number	= {2},
  pages		= {423--443},
  publisher	= {American Political Science Association},
  title		= {Extended Deterrence and the Outbreak of War},
  volume	= {82},
  year		= {1988}
}

