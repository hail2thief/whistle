@Article{	  post2009,
  author	= {Post, J. M. and Ali, F. and Henderson, S. W. and
		  Shanfield, S. and Victoroff, J. and Weine, S.},
  journal	= {Psychiatry-Interpersonal and Biological Processes},
  keywords	= {adolescent development},
  note		= {ISI Document Delivery No.: 429GR Times Cited: 2 Cited
		  Reference Count: 54 Post, Jerrold M. Ali, Farhana
		  Henderson, Schuyler W. Shanfield, Stephen Victoroff, Jeff
		  Weine, Stevan Guilford publications inc New york},
  number	= {1},
  owner		= {cassydorff},
  pages		= {13-31},
  timestamp	= {2011.08.24},
  title		= {The Psychology of Suicide Terrorism},
  volume	= {72},
  year		= {2009},
  abstract	= {This paper reviews current understandings of the
		  psychology of suicide terrorism for psychiatrists and other
		  mental health professionals to help them better understand
		  this terrifying phenomenon. After discussing key concepts
		  and definitions, the paper reviews both group and
		  individual models for explaining the development of suicide
		  terrorists, with an emphasis on "collective identity."
		  Stressing the importance of social psychology, it
		  emphasizes the "normality" and absence of individual
		  psychopathology of the suicide bombers. It will discuss the
		  broad range of terrorisms, but will particularly emphasize
		  terrorism associated with militant Islam. The article
		  emphasizes that comprehending suicide terrorism requires a
		  multidisciplinary approach that includes anthropological,
		  economic, historical, and political factors as well as
		  psychological ones. The paper concludes with a discussion
		  of implications for research, policy, and prevention,
		  reviewing the manner in which social psychiatric knowledge
		  and understandings applied to this phenomenon in an
		  interdisciplinary framework can assist in developing
		  approaches to counter this deadly strategy.}
}

