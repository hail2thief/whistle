  language	= {English},
  number	= {4},
  pages		= {pp. 427-446},
  publisher	= {Blackwell Publishing on behalf of The International
		  Studies Association},
  title		= {A Comparative Analysis of Monadic and Dyadic Perspectives
		  on the 1976 Lebanese Crisis},
  volume	= {28},
  year		= {1984}
}

