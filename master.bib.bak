@Article{	  guess2007,
  author	= {Guess, C. D. and Tuason, M. T. and Teixeira, V. B.},
  journal	= {Journal for the Theory of Social Behaviour},
  keywords	= {Islamic martyrdom suicide missions terrorism culture Iraq
		  palestine ideological thinking suicide terrorism},
  note		= {ISI Document Delivery No.: 234ZS Times Cited: 0 Cited
		  Reference Count: 107 Guess, C. Dominik Tuason, Ma. Teresa
		  Teixeira, Vanessa B. Conference of the Round Table
		  Discussion on Posters on the Hill Apr, 2007 Washington, DC
		  Blackwell publishing Oxford},
  number	= {4},
  owner		= {cassydorff},
  pages		= {415-+},
  timestamp	= {2011.08.24},
  title		= {A cultural-psychological theory of contemporary Islamic
		  martyrdom},
  volume	= {37},
  year		= {2007},
  abstract	= {What political, economic, religious, and emotional factors
		  are involved in a person's decision to kill civilians and
		  military personnel through the sacrifice of his or her own
		  life? Data for this research were secondary analyses of
		  interviews with Islamic martyrs, as well as their leaders'
		  speeches. This investigation into the
		  cultural-psychological explanations for Islamic martyrdom
		  leads to a model explaining a person's decision to carry
		  out the mission as resulting from a combination of four
		  factors: the historical-cultural context, group processes,
		  immediate and anticipated rewards, and mechanisms to
		  eradicate possible doubts and guilt regarding this
		  decision. Compared to existing models, this model is more
		  integrative and focused on the psychological processes
		  involved.}
}

