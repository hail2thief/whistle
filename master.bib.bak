@Article{	  jordan2009,
  author	= {Jordan, J.},
  journal	= {Security Studies},
  keywords	= {decapitation, terrorism},
  owner		= {cassydorff},
  pages		= {719--755},
  publisher	= {Routledge, part of the Taylor \& Francis Group},
  review	= {This is probably one of the most applicable studies for
		  our project). Jordan investigates the effectiveness of
		  leadership decapitation on terrorist groups by assessing
		  the duration of the organization and its tactical capacity
		  following an attack. Using an original data set (1945-2000)
		  Jordan uses measures of group size, age, and organization
		  type as her independent variables and argues that
		  ideological organizations are weaker following decapitation
		  than are religious ones, and younger and smaller
		  organizations are more likely to cease activity following
		  the targeted killing of leader (she measures her DV,
		  effectiveness, in two ways: firstly by group
		  collapse/lifespan and secondly the group's capacity to
		  carry out future attacks following the decapitation). She
		  claims that leadership decapitation is an ineffective
		  counterterrorism strategy because it does not increase the
		  likelihood of the group's collapse. -Can't specifically at
		  whether number of members increases (back lash recruitment)
		  -Predicts individual head leader matters more than upper
		  echelon members. But does quantity matter? Does she account
		  for variation in number of leaders killed?},
  timestamp	= {2012.03.13},
  title		= {When heads roll: assessing the effectiveness of leadership
		  decapitation},
  volume	= {18},
  year		= {2009}
}

