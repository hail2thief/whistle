@Article{	  blundell1998,
  abstract	= {The 1980's tax reforms and the changing dispersion of
		  wages offer one of the best opportunities yet to estimate
		  labor supply effects. Nevertheless, changing sample
		  composition, aggregate shocks, the changing composition of
		  the tax paying population, and discontinuities in the tax
