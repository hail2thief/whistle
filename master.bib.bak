@Article{	  leuthold1988,
  abstract	= {In this study, the median voter model is applied to the
		  problem of predicting state public expenditures in four
		  major categories: human services, elementary and secondary
		  education, higher education, and health services. Demand
		  functions for public services are derived within a formal
		  utility-maximizing model. The resulting model is estimated
		  using time-series data for a representative state and the
		  model is used to predict state spending for fiscal year
		  1985. The results of the estimation are consistent with the
		  predictions of the median voter model and the forecasts
		  correspond closely to those made by the State Bureau of the
		  Budget. In contrast, a naive autoregressive model of state
		  spending performs poorly.},
  author	= {Leuthold, Jane H.},
