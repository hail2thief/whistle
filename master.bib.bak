@Article{	  kruglanski2009,
  author	= {Kruglanski, A. W. and Fishman, S.},
  journal	= {Revista De Psicologia Social},
  keywords	= {Terrorism counterterrorism psychological explanations of
		  terrorism suicide terrorism mortality salience
		  accessibility psychology management persuasion conflict
		  beliefs bombers logic},
  note		= {ISI Document Delivery No.: 441FD Times Cited: 0 Cited
		  Reference Count: 92 Kruglanski, Arie W. Fishman, Shira
		  Fundacion infancia aprendizaje Madrid},
  number	= {2},
  owner		= {cassydorff},
  pages		= {139-162},
  timestamp	= {2011.08.24},
  title		= {What makes terrorism tick? Its individual, group and
		  organizational aspects},
  volume	= {24},
  year		= {2009},
  abstract	= {This paper explorer psychological factors involved in
		  terrorism and counterterrorism on individual, group and
		  organizational levels of analysis. On the individual level,
		  we describe attempts to understand terrorist behavior as a
		  form of psychopathology, and/or as reflecting a unique
		  constellation of personality traits. We also consider
		  whether at the individual level there exists a uniform
		  motivational basis for participating in terrorism. On the
		  group level of analysis, we address the process of shared
		  reality construction, social influence involved in
		  recruitment of new members to terrorist organization, their
		  indoctrination into terrorist ideology, and the use of
		  language in creating terrorism warranting norms. On the
		  organizational level we consider issuer of training,
		  logistics and cost effectiveness as they apply to the
		  decisions to launch terrorist activities, or abstain front
		  their deployment. We conclude by considering the
		  implications for our analysis for possible ways and means
		  for countering terrorism.}
}

