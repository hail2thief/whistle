@Article{	  havardhegre06012004,
  abstract	= {A measure--trade efficiency--that models the extent to
		  which individual economic entities within two countries
		  trade with each other is used to investigate the claim that
		  symmetrical dependence on trade between two states is
		  required for the trade bond to reduce the probability of
		  interstate conflict. This measure is better suited to study
		  this question than existing measures since it is by
		  definition uncorrelated with asymmetries in country size.
		  The relationship between the different conceptions of
		  interdependence and militarized conflict is explored in an
		  expected utility model of trade, distribution of resources,
		  and conflict. For the particular pacifying mechanisms of
		  trade studied here, the model supports the view that trade
		  reduces the incentives for conflict but that this effect is
		  most clearly seen in relatively symmetric dyads. },
  author	= {Hegre, Havard},
  doi		= {10.1177/0022002704263954},
  journal	= {Journal of Conflict Resolution},
  keywords	= {geogvariable},
  number	= {3},
  pages		= {403-429},
  title		= {{Size Asymmetry, Trade, and Militarized Conflict}},
  volume	= {48},
  year		= {2004},
  bdsk-url-1	= {http://dx.doi.org/10.1177/0022002704263954}
}

@Article{	  hawdon2009,
  author	= {Hawdon, J. and Ryan, J.},
  journal	= {Current Sociology},
  keywords	= {community social capital social disorganization terrorism
		  trust social-disorganization theory collective efficacy
		  spatial dynamics civic engagement local capitalism
		  globalization intervention performance sociology violence},
  note		= {ISI Document Delivery No.: 450CU Times Cited: 0 Cited
		  Reference Count: 93 Hawdon, James Ryan, John Sage
		  publications ltd London},
  number	= {3},
  owner		= {cassydorff},
  pages		= {323-343},
  timestamp	= {2011.08.24},
  title		= {Hiding in Plain Sight Community Organization, Naive Trust
		  and Terrorism},
  volume	= {57},
  year		= {2009},
  abstract	= {Terrorism is behavior that is both initiated and sustained
		  by actors within a social environment. That social
		  environment may vary along a continuum from supporting and
		  enabling the behavior, to ignoring or being unaware of the
		  behavior, to actively opposing the behavior. This article
		  applies social disorganization theory, social capital
		  theory and Black's work on terrorism to predict community
		  characteristics likely to provide the anonymity required
		  for the development of terrorist activity in developed
		  nations. Using the Social Capital Community Benchmark
		  Survey and case studies, the article demonstrates how the
		  theory predicts and helps explain why the 9/11 terrorists
		  went undetected even as they lived in the US.}
}

