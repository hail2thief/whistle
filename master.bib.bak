  journal	= {The Journal of conflict resolution},
  month		= jun,
  number	= {2},
  pages		= {363--389},
  pmid		= {22140275},
  title		= {{A New Stress-Based Model of Political Extremism: Personal
		  Exposure to Terrorism, Psychological Distress, and
		  Exclusionist Political Attitudes.}},
  volume	= {53},
  year		= {2009},
  bdsk-url-1	= {http://dx.doi.org/10.1177/0022002709333296}
}

