@Article{	  colaresi:2001,
  abstract	= {The different phases of the leadership long cycle are
		  hypothesized to significantly alter the number of great
		  power rivalries that terminate and initiate. Specifically,
		  the global war phase is expected to "shock" dyads into and
		  out of rivalry. Bivariate and multivariate event history
		  techniques are used to show that periods of capability
		  deconcentration are associated with increased great power
		  rivalry terminations but not initiations. Furthermore,
		  terminations are less likely to occur during phases of
		  systemic capability concentration than in other periods, as
		  the theory predicts. The expectations concerning rivalry
		  initiations are not supported. },
  author	= {Colaresi, Michael},
  doi		= {10.1177/0022002701045005002},
  journal	= {Journal of Conflict Resolution},
  keywords	= {geogvariable},
  number	= {5},
  pages		= {569-593},
  title		= {{Shocks to the System: Great Power Rivalry and the
		  Leadership Long Cycle}},
  volume	= {45},
  year		= {2001},
  bdsk-url-1	= {http://dx.doi.org/10.1177/0022002701045005002}
}

@Article{	  colaresi:2004,
  abstract	= {Varied research traditions suggest that dovish leaders
		  will be thrown out of office under harsh external
		  circumstances. Below, I elaborate a model of rivalry
		  maintenance that draws on and refines the insight from
		  studies of leadership tenure and foreign policy.
		  Specifically, I expect a leader who offers unreciprocated
		  cooperation to a rival (a dove) to be more likely to be
		  deselected from power than a leader that takes a harder
		  line vis-\{\`{a}\}-vis the rival (a hawk). I test
		  expectation using event history techniques and data
		  spanning the 1950-1990 time period and find strong evidence
		  that dovish leaders pay an electoral price within a rivalry
		  context. The findings suggest an internationally contingent
		  domestic incentive to maintain rivalry and conflict over
		  time.},
  author	= {Colaresi, Michael},
  issn		= {00925853},
  journal	= {American Journal of Political Science},
  number	= {3},
  pages		= {pp. 555--570},
  publisher	= {Midwest Political Science Association},
  title		= {{When Doves Cry: International Rivalry, Unreciprocated
		  Cooperation, and Leadership Turnover}},
  volume	= {48},
  year		= {2004}
}

