}

@Article{	  michaelkoch12012005,
  abstract	= {Electoral institutions influence legislators' constituency
		  size and makeup and, as a result, affect the lens that
		  representatives look through to assess the costs of
		  military conflict. Given the uneven distribution of
		  casualties during a conflict, the costs of international
		  violence vary between constituencies and thus affect
		  representatives differently. The authors develop a
		  constituency-based theory of legislator accountability and
		  legislature behavior that predicts when democracies are
		  willing to pay human costs in an interstate conflict and
		  their likelihood of being involved in a dispute. The
		  results suggest that the more diffuse political
		  account-ability, the less likely a state is to get involved
		  in a militarized dispute, but that once involved, the more
		  likely a state will sustain casualties. The authors'theory
		  suggests that choices over the mechanisms of political
		  representation have far-reaching effects on political
		  accountability and foreign policy. },
  author	= {Koch, Michael and Gartner, Scott Sigmund},
  doi		= {10.1177/0022002705281149},
  journal	= {Journal of Conflict Resolution},
  keywords	= {geogvariable},
  number	= {6},
  pages		= {874-894},
  title		= {{Casualties and Constituencies: Democratic Accountability,
		  Electoral Institutions, and Costly Conflicts}},
  volume	= {49},
  year		= {2005},
  bdsk-url-1	= {http://dx.doi.org/10.1177/0022002705281149}
}

@Article{	  michaelmousseau08012000,
  abstract	= {A model is introduced that yields a single parsimonious
		  explanation for a diverse range of political phenomena,
		  including the processes of democratic consolidation and
		  peace among democratic nations. The model predicts
		  democratic values to arise from the norms of contract that
		  are endemic in developed market economies and yields the
		  novel contingent claim that the peace among democratic
		  nations may be a pattern limited to those democracies with
		  developed economies. Analyses of a large number of
		  interstate dyads from 1950 to 1992 show strong support for
		  this hypothesis. It seems that the pacifying impact of
		  democracy is about twice as strong among developed
		  countries compared with other dyads. Among conflict-prone
		  contiguous dyads, the pacifying impact of democracy does
		  not appear statistically significant among the poorest
		  decile of joint democratic dyads. The study demonstrates
		  the wide explanatory power of the simple postulate that
		  social values and political preferences derive from
		  socioeconomic norms. },
  author	= {Mousseau, Michael},
  doi		= {10.1177/0022002700044004004},
  journal	= {Journal of Conflict Resolution},
  keywords	= {geogvariable},
  number	= {4},
  pages		= {472-507},
  title		= {{Market Prosperity, Democratic Consolidation, and
		  Democratic Peace}},
  volume	= {44},
  year		= {2000},
  bdsk-url-1	= {http://dx.doi.org/10.1177/0022002700044004004}
}

