@Article{	  garnham:1976,
  title		= {{Dyadic International War 1816-1965: The Role of Power
		  Parity and Geographical Proximity}},
  author	= {Garnham, David},
  journal	= {The Western Political Quarterly},
  year		= {1976},
  number	= {2},
  pages		= {pp. 231-242},
  volume	= {29},
  abstract	= {This article tests the hypotheses that interstate war is
		  more probable (a) between nation-states of approximately
		  equal national power (power parity) and (b) between
		  geographically proximate nation-states. The data base
		  consists of thirty dyadic interstate wars during the period
		  1816-1965. The analysis employs four indicators of national
		  power: geographical area, population, fuel consumption, and
		  steel production. Hypotheses are tested using the
		  Kolmogorov-Smirnov one-sample test. The proximity
		  hypothesis is confirmed (p&lt;.01). The power parity
		  hypothesis is partially confirmed; except for the steel
		  production analysis, all results are in the predicted
		  direction. The population hypothesis is confirmed
		  (p&lt;.025), and there is considerable support for the
		  geographical area hypothesis (p&lt;.10). The findings
		  justify further dyadic and nondyadic analyses and an effort
		  to develop a power parity theory of interstate war.},
  copyright	= {Copyright {\copyright} 1976 University of Utah},
