  pages		= {796-816},
  title		= {{No Professional Soldiers, No Militarized Interstate
		  Disputes?: A New Question for Neo-Kantianism}},
  volume	= {47},
  year		= {2003},
  bdsk-url-1	= {http://dx.doi.org/10.1177/0022002703258803}
}

