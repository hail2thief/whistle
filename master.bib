@Article{	  obrien2002,
  abstract	= {One way to demonstrate progress in a field of scientific
		  inquiry is to show that factors believed to explain some
		  phenomenon can also be used effectively to predict both its
		  occurrence and its nonoccurrence. This study draws on the
		  state strength literature to identify relevant country
		  macrostructural factors that can contribute to different
		  kinds and levels of intensity of conflict and country
		  instabilities. A pattern classification algorithm-fuzzy
		  analysis of statistical evidence (FASE)-is used to analyze
		  the relationships between country macrostructural factors
		  and historical instances of country instability. A
		  split-sample validation design is used to evaluate the
		  ability of FASE to generate competent predictions, using
		  the standard forecasting performance metrics overall
		  accuracy, recall, and precision. The results demonstrate
		  the potential for FASE to accurately forecast not just the
		  occurrence but also the level of intensity of
		  country-specific instabilities out 5 years with about 80%
		  overall accuracy.},
  author	= {O'Brien, Sean P.},
  copyright	= {Copyright 2002 Sage Publications, Inc.},
  date-added	= {2010-07-01 16:16:06 -0700},
  date-modified	= {2010-07-01 16:17:33 -0700},
  issn		= {00220027},
  journal	= {The Journal of Conflict Resolution},
  jstor_articletype={primary_article},
  jstor_formatteddate={Dec., 2002},
  number	= {6},
  pages		= {791--811},
  publisher	= {Sage Publications, Inc.},
  title		= {Anticipating the Good, the Bad, and the Ugly: An Early
		  Warning Approach to Conflict and Instability Analysis},
  volume	= {46},
  year		= {2002}
}

