@Article{	  lorrain:white:1971,
  abstract	= {The aim of this paper is to understand the interrelations
		  among relations within concrete social groups. Social
		  structure is sought, not ideal types, although the latter
		  are relevant to interrelations among relations. From a
		  detailed social network, patterns of global relations can
		  be extracted, within which classes of equivalently
		  positioned individuals are delineated. The global patterns
		  are derived algebraically through a 'functorial' mapping of
		  the original pattern. Such a mapping (essentially a
		  generalized homomorphism) allows systematically for
		  concatenation of effects through the network. The notion of
		  functorial mapping is of central importance in the 'theory
		  of categories,' a branch of modem algebra with numerous
		  applications to algebra, topology, logic. The paper
		  contains analyses of two social networks, exemplifying this
		  approach. [ABSTRACT FROM AUTHOR]},
  author	= {Lorrain, Fran{{\c c}}ois and White, Harrison C.},
  date-added	= {2010-12-12 12:47:47 -0500},
  date-modified	= {2013-05-24 15:41:19 -0500},
  journal	= {Journal of Mathematical Sociology},
  number	= {1},
  pages		= {49 - 80},
  title		= {Structural Equivalence of Individuals in Social Networks},
  volume	= {1},
  year		= {1971}
}

