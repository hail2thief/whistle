  issn		= {00208833},
  journal	= {International Studies Quarterly},
  jstor_articletype={primary_article},
  jstor_formatteddate={Mar., 1977},
  jstor_issuetitle={Special Issue on International Crisis: Progress and
		  Prospects for Applied Forecasting and Management},
  number	= {1},
  pages		= {225--246},
  publisher	= {Blackwell Publishing on behalf of The International
		  Studies Association},
  title		= {An Examination of Basic and Applied International Crisis
