@Article{	  fischer:etal:2008,
  title		= {Support for resistance among Iraqi students: An
		  exploratory study},
  author	= {Fischer, R. and Harb, C. and Al-Sarraf, S. and Nashabe,
		  O.},
  journal	= {Basic and Applied Social Psychology},
  year		= {2008},
  note		= {ISI Document Delivery No.: 340XO Times Cited: 1 Cited
		  Reference Count: 44 Fischer, Ronald Harb, Charles
		  Al-Sarraf, Sarah Nashabe, Omar Annual Conference of the
		  New-Zealand-Psychological-Society 2004 Wellington, NEW
		  ZEALAND New Zealand Psychol Soc Lawrence erlbaum assoc
		  inc-taylor \& francis Philadelphia},
  number	= {2},
  pages		= {167-175},
  volume	= {30},
  abstract	= {This study examines predictors of expressed support for
		  resistance to U.S.-led forces in Iraq among a sample of 130
		  university students in Baghdad. Based on a review of the
		  literature and common discourse, personality, clash of
		  civilization and social-psychological variables are tested
		  as likely predictors of support for resistance. Data were
		  collected in March 2004 during a period of relative
		  political stability and optimism. Only social-psychological
		  variables significantly predicted support. In particular,
		  national identity, perceived procedural injustice, and
		  higher life satisfaction prior to the U.S.-led invasion
		  were significant predictors. Findings are discussed in
		  relation to media and academic accounts explaining
		  political violence in the current conflict.},
  keywords	= {organizational justice social identity terrorism
		  identification model},
  owner		= {cassydorff},
  timestamp	= {2011.08.24}
}

