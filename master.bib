@Article{	  mizruchi:etal:2006,
  title		= {The Conditional Nature of Embeddedness: A Study of
		  Borrowing by Large U.S. Firms, 1973-1994},
  author	= {Mizruchi, Mark S. and Stearns, Linda Brewster and Marquis,
		  Christopher},
  journal	= {American Sociological Review},
  year		= {2006},
  number	= {2},
  pages		= {310-333},
  volume	= {71},
  abstract	= {Economic and organizational sociologists have increasingly
		  demonstrated that the actions of individuals and firms are
		  affected by the social networks within which they are
		  embedded. In recent years scholars have begun to recognize
		  that the effects of these social networks may vary across
		  populations or types of relations. This article examines
		  the extent to which the effects of interfirm networks on
		  the behavior of firms are historically contingent. Focusing
		  on the level of debt financing among approximately 140
		  large U.S. corporations over a 22-year period, the authors
		  show that the extent to which the firms'use of debt was
		  influenced by those with which they were tied through
		  director interlocks declined over time. The authors argue
		  that this decline in the network effect reflected a shift
		  in the institutional environment within which the firms
		  operated, and that it was driven by three processes: the
		  professionalization of the finance function within the
		  firm, the internalization of financial decision-making, and
		  the increased volatility of the environment. They conclude
		  that corporate financing is socially embedded, but this
		  embeddedness is historically contingent.},
  issn		= {00031224},
  jstor_articletype={primary_article},
  jstor_formatteddate={Apr., 2006},
  owner		= {Audrey},
  publisher	= {American Sociological Association},
  timestamp	= {2009.07.22}
}

