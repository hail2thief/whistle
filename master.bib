@Book{		  hosmer:2001,
  title		= {Operations Against Enemy Leaders},
  author	= {Hosmer, S.T.},
  publisher	= {Rand Corp},
  year		= {2001},
  annote	= {Like Hafez, Pape, and Jordan, Hosmer also finds that
