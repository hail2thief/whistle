@Article{	  lars-erikcederman08012001,
  abstract	= {Computational modeling is used to improve our
		  understanding of how the democratic peace unfolds as a
		  historical process in time and space. Whereas most of the
		  conventional literature interprets the phenomenon as a
		  constant and universal law operating at the state level,
		  the author follows Immanuel Kant and treats democratic
		  cooperation as an emergent macroprocess. The current study
		  explores three causal mechanisms. First, strategic tagging
		  introduces a way for democracies to select out like-minded
		  cooperators. Second, regime-sensitive alliances enable
		  democratic states to defend the gains of cooperation.
		  Third, liberal collective security complements the liberal
		  alignments. Based on these processes, it is possible to
		  "grow" cooperative outcomes in an inhospitable geopolitical
		  environment. Because tagging alone is insufficient,
		  alliances, and sometimes even collective security, are
		  necessary to produce perpetual peace. Such outcomes are
		  characterized by high levels of spatial clustering. },
  author	= {Cederman, Lars-Erik},
  date-added	= {2010-02-08 12:26:05 -0500},
  date-modified	= {2010-02-08 12:26:05 -0500},
  doi		= {10.1177/0022002701045004004},
  journal	= {Journal of Conflict Resolution},
  keywords	= {simulation involving space},
  number	= {4},
  pages		= {470-502},
  title		= {{Modeling the Democratic Peace as a Kantian Selection
		  Process}},
  volume	= {45},
  year		= {2001},
  bdsk-url-1	= {http://dx.doi.org/10.1177/0022002701045004004}
}

